var mongoose=require('mongoose');
var Schema=mongoose.Schema;
var User2Schema=new Schema({
    name:String,
    password:String,
    email:String,
    state:String,
    city:String,
    File:String,
    Dob:Date,
    Gender:String,
    IsDeleted:Boolean
});
module.exports=mongoose.model('User2',User2Schema);

